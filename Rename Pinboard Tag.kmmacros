						<string>#!/usr/bin/python
import pinboard
import os
import sys

pin_user = os.getenv('KMVAR_pin_user')
pin_pass= os.getenv('KMVAR_pin_pass')
old_tag = os.getenv('KMVAR_pin_tag')
new_tag = os.getenv('KMVAR_pin_tag_rename')

